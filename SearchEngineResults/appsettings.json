{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SearchEngine": [
    {
      "Name": "Google",
      "Url": "https://www.google.com/search?q={0}&num={1}",
      "FilterExpression": "<div class=\"kCrYT\" *?><a href=.*?</div>",
      "Count": 100
    },
    {
      "Name": "Bing",
      "Url": "https://www.bing.com/search?q={0}&count={1}",
      "FilterExpression": "li class=\"b_algo\".*?><h2><a href=.*?</li>",
      "Count": 100
    },
    {
      "Name": "Yahoo",
      "Url": "https://au.search.yahoo.com/search?p={0}&n={1}",
      "FilterExpression": "<span class=\" fz-ms .*?>.*?</span>",
      "Count": 100
    }
  ]
}
